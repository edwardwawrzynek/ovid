fn fact(n i64) -> i64 {
    if n <= 1 {
        return 1
    } else {
        return n * fact(n - 1)
    }
}

fn run(n i32) {
    if(n < 20) {
        bootstrap:print_num(fact(n))
        run(n + 1)
    }
}

pub fn main() -> i32 {
    run(0)
    return 0
