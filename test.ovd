// compute n!

pub fn fact(n i64) -> i64 {
    if n <= 1 {
        return 1
    } else {
        return n * fact(n - 1)
    }

}

pub fn addr(a i32) -> i32 {
    return *&a
}

pub fn test(a i32) -> i32 {
    val b = a
    val c = &b
    return *c
}

pub fn escape(a i32) -> *i32 {
    return &a
}

fn main() -> i32 {
    return *escape(23)
}