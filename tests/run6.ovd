pub fn main() -> i32 {
    val a = -2
    val b = 7
    if (1 + 1) * 2 != a * -2 {
        return 255
    }
    if -(1 + 1) != a {
        return 255
    }
    if (15 & b) != b {
        return 255
    }
    if (34 | b) != b + 32 {
        return 255
    }
    if ((b + 1) ^ 67) != 61 + (b << 1) {
        return 255
    }

    return 0
}

